{
  "name": "hyrax",
  "private": true,
  "repository": "git@github.com:samvera/hyrax.git",
  "dependencies": {
    "@babel/traverse": ">=7.23.2",
    "browserify-sign": ">=4.2.2",
    "@rails/webpacker": "5.4.4",
    "babel": "^6.23.0",
    "jquery": ">=3.5.0",
    "follow-redirects": ">=1.15.4",
    "minimist": ">=1.2.6",
    "node-fetch": ">=2.6.7",
    "node-forge": ">=1.3.0",
    "nth-check": ">=2.0.1",
    "popper.js": "^1.16.1",
    "postcss": ">=8.4.31",
    "tough-cookie": ">=4.1.3",
    "three": ">=0.125.0",
    "universalviewer": "^3.0.36",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "xmldom": ">=0.6.0",
    "@xmldom/xmldom": ">=0.8.4",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "preinstall": "rm -rf ./public/uv",
    "postinstall": "yarn run uv-install && yarn run uv-config",
    "uv-install": "shx cp -r ./node_modules/universalviewer/dist ./public/uv",
    "uv-config": "shx cp ./config/uv/uv.html ./public/uv/uv.html & shx cp ./config/uv/uv-config.json ./public/uv/"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "shx": "^0.3.2",
    "webpack-dev-server": "^3"
  }
